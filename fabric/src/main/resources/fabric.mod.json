{
  "schemaVersion": 1,
  "id": "create_lt",
  "version": "${version}",
  "name": "Create: Less Ticking",
  "description": "A tiny performance mod, decreasing the rate some Create components tick at",
  "authors": [
    "Dadamalda"
  ],
  "contact": {
    "homepage": "https://modrinth.com/project/create-lt",
    "sources": "https://github.com/DadamaldaDad/Create-Less-Ticking"
  },
  "license": "MIT",
  "icon": "assets/create_lt/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.dadamalda.create.less_ticking.fabric.CLTFabric"
    ]
  },
  "mixins": [
    "examplemod.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "*"
  }
}
